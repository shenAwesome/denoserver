<service> 
  <id>DenoServer</id>  
  <description>
	Starts DenoServer.
  </description>
  <executable>D:\code\github\denoserver\core\deno.exe</executable>
  <arguments>run --allow-net --watch ..\Main.ts --config-port=9000</arguments>
  <log mode="roll-by-size">
	  <sizeThreshold>10240</sizeThreshold>
	  <keepFiles>8</keepFiles>
	</log>
</service>